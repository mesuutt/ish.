{
    "name": "Resposive Test in place",
    "description": "Test responive design of any website with one click. No needs any permission.",
    "manifest_version": 2,
    "version": "0.1",
    "content_scripts": [{
        "js": [ "jquery-1.10.2.min.js", "js/data-saver.js", "js/url-handler.js", "js/init.js", "contentscript.js"],
        "css": [ "styles.css" ],
        "matches": ["http://*/*", "https://*/*"]
    }],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}
